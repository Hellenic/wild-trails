{
  "name": "wild-trails",
  "description": "Wild Trails is a real-world adventure game that combines orienteering, puzzle-solving, and geocaching, where players navigate through wilderness using clues to find their final destination within a time limit.",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "predeploy": "npm run test && npm run build",
    "deploy": "vercel deploy",
    "supabase": "supabase",
    "db:login": "supabase login",
    "db:setup": "supabase link --project-ref ntksyfdomphadaqzciwd",
    "db:create-migration": "supabase migration new",
    "db:generate-types": "supabase gen types typescript --project-id \"ntksyfdomphadaqzciwd\" --schema public > types/database.types.ts",
    "db:apply-migrations": "supabase db push",
    "test": "jest",
    "test:watch": "jest --watch",
    "pretest:e2e": "playwright install",
    "test:e2e": "playwright test -- e2e/"
  },
  "dependencies": {
    "@supabase/ssr": "0.5.2",
    "@supabase/supabase-js": "2.47.10",
    "@turf/turf": "7.3.1",
    "leaflet": "1.9.4",
    "next": "16.0.7",
    "osmtogeojson": "3.0.0-beta.5",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-leaflet": "5.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.1",
    "@types/geojson": "^7946.0.15",
    "@types/jest": "^29.5.5",
    "@types/leaflet": "^1.9.15",
    "@types/node": "^24",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "jest": "^29.7.0",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "supabase": "^2.1.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vercel": "49.1.1"
  },
  "overrides": {
    "@xmldom/xmldom": "0.8.4",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3"
  }
}
